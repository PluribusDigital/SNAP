<p id="notice"><%= notice %></p>

<h1>Listing Retailers</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Longitude</th>
      <th>Latitude</th>
      <th>Address1</th>
      <th>Address2</th>
      <th>City</th>
      <th>State</th>
      <th>Zip5</th>
      <th>Zip4</th>
      <th>Country</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @retailers.each do |retailer| %>
      <tr>
        <td><%= retailer.name %></td>
        <td><%= retailer.longitude %></td>
        <td><%= retailer.latitude %></td>
        <td><%= retailer.address1 %></td>
        <td><%= retailer.address2 %></td>
        <td><%= retailer.city %></td>
        <td><%= retailer.state %></td>
        <td><%= retailer.zip5 %></td>
        <td><%= retailer.zip4 %></td>
        <td><%= retailer.country %></td>
        <td><%= link_to 'Show', retailer %></td>
        <td><%= link_to 'Edit', edit_retailer_path(retailer) %></td>
        <td><%= link_to 'Destroy', retailer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Retailer', new_retailer_path %>
